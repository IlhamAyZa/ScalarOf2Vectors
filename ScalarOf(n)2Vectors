#include<stdio.h>
#include <cmath>
using namespace std;

int main()
{
    int x, y, n;
    float scalX, scalY, scal;

    printf("Enter vector space : ");
    scanf("%d", &n);

    for (int i = 0; i < n, i++){
        printf("Enter X and Y for the vector => \n");

        printf("X : ");
        scanf("%d", &x);
        scalX += x;

        printf("Y : ");
        scanf("%d", &y);
        scalY += y;

    }

    scal = sqrt(pow(scalX, 2)+ pow(scalY, 2));
    printf("Scalar : %f", scal);
}
